{
  "name": "alaskaki-backend",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "start": "node dist",
    "dev": "nodemon --exec babel-node --presets=@babel/preset-env -- src/index.js",
    "build": "babel --delete-dir-on-start src/ --out-dir dist/ --presets=@babel/env",
    "lint": "eslint src",
    "format": "prettier --check --write \"!dist/*\" \"**/*.js\""
  },
  "keywords": [],
  "author": "Anang Fachreza <fachrezanang@gmail.com> (https://fcrezza.com)",
  "license": "ISC",
  "dependencies": {
    "config": "^3.3.3",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.1",
    "@babel/node": "^7.13.0",
    "@babel/preset-env": "^7.13.5",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^4.3.8",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format"
    }
  }
}
